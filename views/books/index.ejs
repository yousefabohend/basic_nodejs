<h2>Search Books</h2>
<form action="/books" method="GET">
   <div>
        <label>Title</label>
        <input type="text" name="title" value="<%= searchOptions.title%>">
    </div>
    <div>
        <label>Publish After</label>
        <input type="text" name="publishAfter" value="<%= searchOptions.publishAfter%>">
    </div>
    <div>
        <label>Publish Before</label>
        <input type="text" name="publishBefore" value="<%= searchOptions.publishBefore%>">
    </div>
    
    <button type="submit">Search</button>
</form>
<br>
<div>
<% books.forEach(book => { %>
    <div>
   <img height="150" width="100" src="<%= book.coverImagePath %>" alt="">
    </div>
    
<%});%>
</div>